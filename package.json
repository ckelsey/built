{
    "name": "builtjs",
    "version": "0.2.6",
    "main": "dist/main.js",
    "scripts": {
        "start": "NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
        "build": "NODE_ENV=production webpack --progress --config ./webpack.config.js",
        "check-types": "tsc",
        "type-check": "tsc --noEmit",
        "build:types": "tsc --emitDeclarationOnly",
        "build:babel": "babel src --out-dir dust --extensions \".ts\" --source-maps inline",
        "demo": "browser-sync . --port '5555' --index ./demo.html",
        "test": "npx codeceptjs run --steps",
        "jest": "jest"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ckelsey/built.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/preset-env": "^7.4.1",
        "@babel/preset-typescript": "^7.3.3",
        "@epegzz/sass-vars-loader": "^5.0.0",
        "@types/node": "^12.7.1",
        "autoprefixer": "^9.6.1",
        "babel-loader": "^8.0.5",
        "browser-sync": "^2.26.3",
        "codeceptjs": "^2.3.1",
        "compression-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "css-to-string-loader": "^0.1.3",
        "eslint": "^6.2.1",
        "file-loader": "^4.1.0",
        "g": "^2.0.1",
        "gulp": "^4.0.0",
        "hard-source-webpack-plugin": "^0.13.1",
        "html-loader": "^0.5.5",
        "html-minifier": "^4.0.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.12.0",
        "postcss-loader": "^3.0.0",
        "puppeteer": "^1.20.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.0",
